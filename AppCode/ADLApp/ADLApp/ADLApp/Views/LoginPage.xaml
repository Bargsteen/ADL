<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ADLApp.Views.LoginPage">

  <StackLayout>
    <Label Text="Login"></Label>
    <Entry x:Name="UsernameEntry"   Placeholder="Brugernavn"></Entry>
    <Entry x:Name="PasswordEntry" Placeholder="Adgangskode"></Entry>
    <Button Text="Login" Clicked="LoginButton_OnClicked"></Button>
  </StackLayout>
  

</ContentPage>